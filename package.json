{
	"name": "PDACs",
	"version": "1.3.0",
	"description": "PDAC Manager - Thursday",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "rollup -c",
		"autobuild": "rollup -c -w",
		"start": "nvm use v14.15.0 && electron .",
		"start:dev": "concurrently \"npm run autobuild\" \"ELECTRON_RELOAD=yes electron .\"",
		"start:browser": "concurrently \"npm run autobuild\" \"browser-sync public -w --no-ghost-mode\"",
		"package": "electron-packager . --out=dist $npm_package_name-$npm_package_version --all --ignore=rollup.config.js --ignore=.gitignore --overwrite && npm run copy && npm run open",
		"copy": "cp -rf dist/*/*.app /Applications",
		"open": "open /Applications/$npm_package_name-$npm_package_version.app",
		"rebuild": "electron-rebuild -f -w midi"
	},
	"engines": {
		"node": "14.x"
	},
	"author": "Gilbert Sinnott <g@sinnott.cc>",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.9.6",
		"@tsconfig/svelte": "^1.0.10",
		"babel-loader": "^8.1.0",
		"browser-sync": "^2.26.7",
		"concurrently": "^5.3.0",
		"electron": "^10.1.5",
		"electron-packager": "^15.1.0",
		"electron-reload": "^1.5.0",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.32.1",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-svelte": "^6.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.22.2",
		"svelte-check": "^1.1.10",
		"svelte-loader": "^2.13.6",
		"svelte-preprocess": "^4.5.2",
		"typescript": "^4.0.5"
	},
	"dependencies": {
		"@rollup/plugin-alias": "^3.1.1",
		"@rollup/plugin-typescript": "^6.1.0",
		"axios": "^0.21.0",
		"dayjs": "^1.9.7",
		"electron-context-menu": "^2.3.0",
		"electron-promise-ipc": "^2.2.1",
		"electron-rebuild": "^2.3.4",
		"expand-home-dir": "^0.0.3",
		"fs": "^0.0.1-security",
		"fs-promise": "^2.0.3",
		"highlight.js": "^10.3.2",
		"midi": "^1.0.0",
		"moment": "^2.29.1",
		"node-osc": "file:./node-osc",
		"node-sass": "^4.14.1",
		"simple-git": "^2.21.0",
		"speed-date": "^1.0.0",
		"svelte-hash-router": "^1.0.1",
		"svelte-preprocess-css-global": "^0.0.1",
		"svelte-preprocess-sass": "^0.2.0",
		"websocket": "^1.0.33",
		"websockets": "^0.2.0"
	}
}
